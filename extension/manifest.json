{
	"manifest_version": 2,
	"name": "Subtitle Generator",
	"version": "0.1",
	"background": {
		"scripts": ["dist/background.js"]
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{"matches": [
			"<all_urls>"
		],
		"js": ["dist/content.js"]
		}
	],
	"web_accessible_resources": ["dist/*"],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
